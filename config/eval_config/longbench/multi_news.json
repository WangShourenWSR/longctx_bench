{
    "eval_params": {
        "dataset": "multi_news",
        "dataset_path": "dataset/longbench",
        "instruction": "You are given several news passages. Write a one-page summary of all news. \n\nNews:\n{context}\n\nNow, write a one-page summary of all the news.\n\nSummary:",
        "instruction_position": "prefix",
        "max_new_tokens": 512,
        "eval_metrics": [
            "rouge_score"
        ]
    },
    "management": {
        "sub_dir": {
            "input_config": "input_config/",
            "raw_results": "raw_results.json",
            "result_vis": "result_vis.png",
            "output_config": "output_config.json"
        }
    }
}
